    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch from file folder",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${fileDirname}",
            "env": {
                "SC_TOKEN": "token",
                "SC_ORGANIZATION": "dodopizza"
            }
        },
        {
            "name": "Launch file",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${file}"
        },
    ]
}